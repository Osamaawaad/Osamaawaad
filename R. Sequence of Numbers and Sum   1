#include <iostream>
 
using namespace std;
 
int main()
{
    long long  n,m;
    long long  sum=0;
    long long  sum2=0;
    while(cin>>n>>m)//5 6
    {
        if(n<=0||m<=0)
        {
            return 0;
        }
        if(m>n){
           int temp=m;
           m=n;
           n=temp;
        }
 
            for(int i=m; i<=n; i++)
            {
 
                cout<<i<<" ";
                sum=sum+i;
            }
            cout<<"sum ="<<sum<<endl;
            sum=0;
 
 
 
 
 
    }
}
 
 
 
 
 

#include <iostream>

using namespace std;

int main()
{
   int n,m;

   for(int i=1;i<=3;i++){


    cin>>n>>m;
 if(n<=0||m<=0){
    break;
 }
 } if(n<m){
        for(int i=m;i<=n;i++){
        int sum=0;
        cout<<i<<" ";
        sum=sum+i;


cout<<"sum = "<<sum<<endl;
        }


    }else if(m<n){
 for(int i=n;i<=m;i++){
    int sum2=0;
    cout<<i<<" ";
    sum2=sum2+i;

    cout<<"sum2 = "<<sum2<<endl;
    }



    }
}




#include <iostream>
 
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    int f=1;
 
    while(t--){
        if(f%4==0){
            cout<<"PUM"<<endl;
            f++;//5
        }
        cout<<f<<' ';//1
        f++;//2
        cout<<f<<' ';//2
        f++;
        cout<<f<<' ';//3
        f++;//4
 
    }
    cout<<"PUM";
    return 0;
}
